<module>
	<rawname>motif</rawname>
	<name>Google Voice/Chan Motif</name>
	<publisher>Sangoma Technologies Corporation</publisher>
	<license>GPLv3+</license>
	<licenselink>https://www.gnu.org/licenses/gpl-3.0.txt</licenselink>
	<type>setup</type>
	<repo>unsupported</repo>
	<category>Connectivity</category>
	<version>15.0.1</version>
	<description>
		Manage Google Voice Trunks with Chan Motif
	</description>
	<changelog>
		*15.0.1* Backup 15
	</changelog>
	<depends>
		<engine>asterisk 11</engine>
		<module>sipsettings</module>
	</depends>
    <database>
        <table name="motif">
            <field name="id" type="integer" primarykey="true" autoincrement="true"/>
            <field name="phonenum" type="string" length="12"/>
            <field name="username" type="string" length="100"/>
            <field name="password" type="string" length="150"/>
            <field name="refresh_token" type="string" length="150"/>
            <field name="oauth_secret" type="string" length="150"/>
            <field name="oauth_clientid" type="string" length="150"/>
            <field name="type" type="string" length="50" default="googlevoice"/>
            <field name="settings" type="blob"/>
            <field name="statusmessage" type="string" length="50"/>
            <field name="authmode" type="string" length="50"/>
            <field name="priority" type="integer" default="127"/>
        </table>
    </database>
	<menuitems>
		<motif>Google Voice (Motif)</motif>
	</menuitems>
	<supported>
		<version>15.0</version>
	</supported>
</module>
